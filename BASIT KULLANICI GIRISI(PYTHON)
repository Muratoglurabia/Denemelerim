print("""
      ****************
      KULLANICI GIRISI
      ****************
""")
kullaniciAdi= "rabia"
sifre ="1234"

a=input("Kullanici adini giriniz: ")
b=input("Sifrenizi giriniz: ")

if (kullaniciAdi == a and sifre != b):
    print("Hatali sifre girdiniz..")
elif (kullaniciAdi != a and sifre== b):
    print("Hatali kullanici adi girdiniz..")
elif (kullaniciAdi != a and sifre != b):
    print("Kullanici adi ve sifre hatali..")
else:
    print("Giris basarili..")
